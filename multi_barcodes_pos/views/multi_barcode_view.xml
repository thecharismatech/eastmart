<record id="view_multi_barcode_tree" model="ir.ui.view">
    <field name="name">multi.barcode.tree</field>
    <field name="model">multi.barcode.products</field>
    <field name="arch" type="xml">
        <tree string="Multi Barcodes">
            <field name="product_multi_id"/>
            <field name="multi_barcode"/>
        </tree>
    </field>
</record>

<record id="action_multi_barcode" model="ir.actions.act_window">
    <field name="name">Multi Barcodes</field>
    <field name="res_model">multi.barcode.products</field>
    <field name="view_mode">list,form</field>
</record>
